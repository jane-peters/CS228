public class ItemSafe<T> 
{
	int maxNum;
	Array<T> seedArr;
	private Object isafe;
	
	/**
	 * @param maxNum
	 * @param seedArr
	 */
	public ItemSafe(int maxNum, Array<T> seedArr) {
		this.maxNum = maxNum;
		this.seedArr = seedArr;
		
		isafe = Arrays.copyOf(seedArr, maxNum);
	}

